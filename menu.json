  "  Setup": {
    "  Audio": {
      "command": "$TERMINAL --class=Wiremix -e wiremix"
    },
    "  Wifi": {
      "command": "rfkill unblock wifi && omarchy-launch-wifi"
    },
    "󰂯  Bluetooth": {
      "command": "rfkill unblock bluetooth && blueberry"
    },
    "󰍹  Monitors": {
      "command": "open_in_editor ~/.config/hypr/monitors.conf"
